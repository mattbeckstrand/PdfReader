{
  "name": "pdf-ai-reader",
  "version": "0.1.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:compile:main": "tsc -p tsconfig.electron.json",
    "electron:compile:preload": "tsc -p tsconfig.preload.json",
    "electron:compile": "npm run electron:compile:main && npm run electron:compile:preload",
    "electron:dev": "npm run electron:compile && VITE_DEV_SERVER_URL=http://localhost:5173 electron ./dist-electron/main.js",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.58.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.22",
    "lucide-react": "^0.544.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "zeroentropy": "^0.1.0-alpha.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
